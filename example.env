# QR Kiosk Scanner Configuration

# Server URL for QR scan endpoint
export QR_SERVER_URL=http://localhost:3000/api/kiosk/v1/qrscan

# Temple ID - Unique identifier for the temple
export TEMPLE_ID=temple_001

# Kiosk ID - Unique identifier for this kiosk machine
export KIOSK_ID=kiosk_main_entrance

# Thermal Printer Device Path
export PRINTER_PATH=/dev/usb/lp0
